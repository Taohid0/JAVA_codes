import java.util.*;
import java.math.*;

public class Main {
           public static void main (String [] args)
           {
        	   Scanner sc = new Scanner(System.in);
        	   int a;
        	   BigDecimal b;
        	   
        	   while(sc.hasNext())
        	   {
        		   a =sc.nextInt();
        		   b = sc.nextBigDecimal();
        		   b = b.pow(a);
        		   
        		   System.out.println(b);
        	   }
           }
}
