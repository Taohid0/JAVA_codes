package multiThread3;

public class CurrentThreadDemo {
                      public static void main(String args[]){
                    	  
                    	  Thread t = Thread.currentThread();
                    	  
                    	  System.out.println(t);
                    	  
                    	  t.setName("my thread");
                    	  
                    	  System.out.println(t)
;                    	  		
                      }
}
