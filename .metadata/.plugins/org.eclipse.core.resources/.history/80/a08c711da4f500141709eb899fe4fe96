package file;

import java.io.*;

public class CopyFile {
   public static void main(String args[])
   {
      FileInputStream in = null;
      FileOutputStream out = null;

      try {
         in = new FileInputStream("d:\\input.txt");
         out = new FileOutputStream("d:\\output.txt");
         
         int c;
         while ((c = in.read()) != -1) {
            out.write(c);
         }
      }
      catch(IOException e){
	
}}}