import java.util.*;
import java.math.*;

public class Main {
    public static void main (String args[])
    {
    	Scanner sc = new Scanner(System.in);
    	int test;
    	BigDecimal b1,b2;
    	
    	test =sc.nextInt();
    	while(test-->0)
    	{
    		b1 = sc.nextBigDecimal();
    		b2 = sc.nextBigDecimal();
    		
    		System.out.println(b1.add(b2.toPlainString());
    	}
    }
}
