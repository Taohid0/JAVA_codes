package multiThread;

public class Multiple implements Runnable  {
                                
                	String threadName;
	
	               // Multiple(String threadName){
	              //  	this.threadName = threadName;
	              //  }
	                
	                public void run(){
	                	for(int i = 5;i>=0;i--){
	                		System.out.println(threadName + " : " + i);
	                	}
	               
	                System.out.println(threadName + " exiting");
} }
