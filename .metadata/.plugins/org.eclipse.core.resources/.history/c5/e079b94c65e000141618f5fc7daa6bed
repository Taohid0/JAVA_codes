package generics;

public class GenDemo {
              public static void main (String args[]){
            	  
            	  Gen<Integer>iOb;
            	  
                  iOb = new Gen<Integer>(88);
                  
                  iOb.showType();
                  
                  int v  = iOb.getOb();
                  
                  System.out.println(v);
                  
                  Gen<String>sOb = new Gen<String>("String class");
                  iOb.showType();
                  
                  String s = sOb.getOb();
                  
                  System.out.println(s);
              }
}
