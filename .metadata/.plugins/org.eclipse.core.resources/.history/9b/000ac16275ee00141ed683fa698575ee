package multiThread;

public class Thread1 implements Runnable{
                  
	Thread t;
	
	  Thread1(){
		  
	  }
	   public void run(){
		   try{
			   for(int i = 5;i>0;i--){
				   System.out.println("child thread" +i);
				   Thread.sleep(200);
			   }
			   
		   }
		   catch (InterruptedException e){
			   
		   }
		   System.out.println("exiting child thread");
	   }
}
